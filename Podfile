platform :ios, '10.0'
use_frameworks!

def dandelion_pods
  use_frameworks!
  pod 'Alamofire'
  pod 'AlamofireImage', '~> 3.3'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxSwiftUtilities'
  pod 'Moya'
  pod 'Moya/RxSwift'
  pod 'SwiftyJSON'
  pod 'TPKeyboardAvoiding'
end

target 'Dandelion' do
  dandelion_pods
end
